@model IEnumerable<UpsaMe_API.Models.CalendlyEvent>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EventUri)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InviteeUri)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartUtc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndUtc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RawJson)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EventUri)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InviteeUri)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartUtc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndUtc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RawJson)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
